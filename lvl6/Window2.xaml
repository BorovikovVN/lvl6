<Window x:Class="lvl6.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lvl6"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="800">


    <Grid>

        <StackPanel>
            <TextBox Name="TextBox1" Style="{StaticResource DataTriggerTextBox}"></TextBox>
            <TextBox Name="TextBox2" Style="{StaticResource TriggerTextBox}"></TextBox>
            <Button Name="Button1" Style="{StaticResource EventTriggerButton}"></Button>
            <Button Name="Button2" Style="{StaticResource ButtonMultitrigger}">adsadsads</Button>
        </StackPanel>

    </Grid>
</Window>
